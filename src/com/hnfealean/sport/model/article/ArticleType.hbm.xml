<?xml version="1.0" encoding="ISO-8859-1"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
  <class table="t_articletype" name="com.hnfealean.sport.model.article.ArticleType">
    <id access="field" name="id">
      <generator class="native"/>
    </id>
    <property name="name" length="20" access="field"/>
    <property name="templateUrl" access="field"/>
    <property name="title" access="field"/>
    <property name="keywords" access="field"/>
    <property name="description" access="field"/>
  </class>
</hibernate-mapping>
