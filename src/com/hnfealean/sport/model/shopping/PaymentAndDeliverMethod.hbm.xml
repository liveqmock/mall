<?xml version="1.0" encoding="ISO-8859-1"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
  <class table="t_payment_and_deliver_method" name="com.hnfealean.sport.model.shopping.PaymentAndDeliverMethod">
    <id access="field" name="id">
      <generator class="native"/>
    </id>
    <property name="deliverMethod" access="field"/>
    <property name="timeLimit" access="field"/>
    <property name="messageRequire" access="field"/>
    <property name="deliverTypeName" access="field"/>
    <property name="paymentMethod" access="field"/>
    <property name="paymentTypeName" access="field"/>
  </class>
</hibernate-mapping>
