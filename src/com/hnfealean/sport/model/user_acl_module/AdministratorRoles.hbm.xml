<?xml version="1.0" encoding="ISO-8859-1"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
  <class table="t_administratorroles" name="com.hnfealean.sport.model.user_acl_module.AdministratorRoles">
    <id access="field" name="id">
      <generator class="native"/>
    </id>
    <many-to-one column="roleId" access="field" name="role"/>
    <many-to-one access="field" name="administrator"/>
    <property name="orderNo" access="field"/>
  </class>
</hibernate-mapping>
