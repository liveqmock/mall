<?xml version="1.0" encoding="ISO-8859-1"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
  <class table="t_purserecord" name="com.hnfealean.sport.model.user_acl_module.PurseRecord">
    <id access="field" name="id">
      <generator class="native"/>
    </id>
    <property name="inOrOut" access="field"/>
    <property name="changeMethod" access="field"/>
    <property name="changeDealRecordNum" access="field"/>
    <property name="changeAmount" access="field"/>
    <many-to-one access="field" name="purse"/>
  </class>
</hibernate-mapping>
