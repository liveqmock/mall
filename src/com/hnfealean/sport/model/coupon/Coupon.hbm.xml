<?xml version="1.0" encoding="ISO-8859-1"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
  <class table="t_coupon" name="com.hnfealean.sport.model.coupon.Coupon">
    <id length="64" access="field" name="id">
      <generator class="assigned"/>
    </id>
    <property name="name" length="64" access="field"/>
    <property name="discountPrice" access="field"/>
    <property name="limitTimes" access="field"/>
    <property name="maxTimes" access="field"/>
    <property name="usedTimes" access="field"/>
    <property name="avaliable" access="field"/>
  </class>
</hibernate-mapping>
